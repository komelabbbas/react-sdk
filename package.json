{
  "name": "@crater/react-sdk",
  "private": false,
  "version": "0.0.3",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && npx tailwindcss build ./src/styles/tailwind.css -o ./dist/index.css",
    "preview": "vite preview",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "eslint --fix 'src/**/*.{jsx,ts,tsx}'",
    "format": "prettier --write src//**/*.{ts,tsx,css} --config ./.prettierrc"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "yarn  eslint",
      "yarn  format"
    ]
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/crater-react-sdk.es.js",
      "require": "./dist/crater-react-sdk.umd.js"
    }
  },
  "main": "./dist/crater-react-sdk.umd.js",
  "module": "./dist/crater-react-sdk.es.js",
  "types": "./dist/index.d.ts",
  "devDependencies": {
    "@types/node": "^18.7.6",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.34.0",
    "@typescript-eslint/parser": "^5.34.0",
    "@vitejs/plugin-react": "^2.1.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.31",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.3.5",
    "typescript": "^4.6.4",
    "unplugin-auto-import": "^0.16.7",
    "vite": "^3.1.0",
    "vite-plugin-dts": "^1.4.1",
    "vite-tsconfig-paths": "^3.5.0"
  },
  "peerDependencies": {
    "react": "16.8.0 || >=17.x",
    "react-dom": "16.8.0 || >=17.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/crater-invoice/react-sdk"
  },
  "keywords": [],
  "author": "Crater",
  "bugs": {
    "url": "https://github.com/crater-invoice/react-sdk/issues"
  },
  "homepage": "https://github.com/crater-invoice/react-sdk#readme",
  "license": "MIT",
  "dependencies": {
    "@heroicons/react": "^2.0.18",
    "classnames": "^2.3.2",
    "i18next": "^23.7.6",
    "react-i18next": "^13.5.0"
  }
}
